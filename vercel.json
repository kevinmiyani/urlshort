{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },

    {
      "src": "my-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "my-frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "my-frontend/build/$1"
    }
  ]
}
