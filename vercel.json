{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },

    {
      "src": "my-frontend/package.json",
      "use": "@vercel/static-build",
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "my-frontend/build/$1"
    }
  ]
}
